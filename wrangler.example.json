{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "grasspush",
  "compatibility_date": "2025-02-04",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "pages_build_output_dir": ".vercel/output/static",
  "observability": {
    "enabled": true
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "grasspush",
      "database_id": "${D1_DATABASE_ID}",
      "migrations_dir": "./drizzle"
    }
  ]
}